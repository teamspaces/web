<!doctype html>
<html class="" lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="description" content="Describe Spaces here.">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= csrf_meta_tags %>

    <title>Spaces: Knowledge Management for Teams</title>

    <link rel="icon" href="">
    <%= stylesheet_link_tag 'application', media: 'all' %>

    <script src="https://use.typekit.net/iec2bxy.js"></script>
    <script>try{Typekit.load({ async: true });}catch(e){}</script>
    <%= javascript_include_tag 'application' %>
    <%= render "layouts/google_analytics" %>
  </head>

  <body class="body-no-topbar <%= content_for :body_class %>">
    <% if notice %>
      <div class="message message-notice"><%= notice %></div>
    <% end %>

    <% if alert %>
      <div class="message message-alert"><%= alert %></div>
    <% end %>

    <%= yield %>
  </body>
</html>
