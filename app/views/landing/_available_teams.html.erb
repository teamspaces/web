<!-- This partial is shown on the default subdomain / without current user -->

<ul>
  <% available_users.teams.each do |team| %>

    <li>
      <%= image_tag team.logo_url(:image_32) %>
      <%= link_to team.name, login_into_team_url(subdomain: team.subdomain), target: "blank" %>
    </li>

  <% end %>
</ul>
